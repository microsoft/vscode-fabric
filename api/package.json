{
  "name": "@microsoft/vscode-fabric-api",
  "publisher": "Microsoft Corporation",
  "displayName": "Microsoft Fabric - APIs for VS Code",
  "description": "Contains APIs for interacting with Microsoft Fabric and VS Code Fabric extensions",
  "version": "0.31.0-preview",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "watch": "tsc -p . -w",
    "test:compile": "tsc -p tsconfig.test.json",
    "pretest:unit": "npm run test:compile",
    "test:unit": "vscode-test --label unit",
    "lint:check": "eslint \"src/**/*.ts\" \"test/**/*.ts\" -c ../.eslintrc.js",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\" -c ../.eslintrc.js --fix",
    "clean": "npm exec rimraf lib out",
    "clean:all": "npm run clean && npm exec rimraf node_modules"
  },
  "dependencies": {
    "@azure/core-rest-pipeline": "^1.14.0",
    "@vscode/extension-telemetry": "^0.9.0"
  },
  "devDependencies": {}
}
