{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2020",
		"strict": true,   /* enable all strict type-checking options */
		"sourceMap": true,
		"allowSyntheticDefaultImports": true, // Allow default imports from modules with no default export, like import decomp from 'decompress';
		"lib": [
			"ES2020", "DOM"
		],
		"baseUrl": "./",
		"resolveJsonModule": true
	}
}
